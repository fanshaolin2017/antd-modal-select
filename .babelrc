{
    "presets": [
        "es2015",
        "react",
        "stage-0",
        ["env", {
            "targets": {
                "browsers": [">0.25%", "ie >= 8"]
            },
            "modules": false
        }]
        
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ],
        "transform-object-rest-spread",
        "react-html-attrs",
        "syntax-dynamic-import",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-assign",
        "lodash"
       
    ]
}